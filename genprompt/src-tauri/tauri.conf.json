{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "promptwtf",
  "version": "0.1.0",
  "identifier": "com.promptwtf.app",
  "build": {
    "frontendDist": "../",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "promptwtf",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "minWidth": 800,
        "minHeight": 600,
        "url": "http://localhost:3000/desktop",
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "python/*.py",
      "python/*.txt",
      "python/data/*.json",
      "python/data/embedded_subset/*.faiss",
      "python/data/embedded_subset/*.pkl"
    ],
    "copyright": "Â© 2025 promptwtf",
    "category": "DeveloperTool",
    "shortDescription": "AI Image Analysis Tool",
    "longDescription": "A desktop application that analyzes AI-generated images to reveal the prompts, models, and parameters used to create them."
  }
}
